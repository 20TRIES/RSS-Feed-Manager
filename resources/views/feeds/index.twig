<!DOCTYPE html>
<html>
    <head>
        <title>{{ title }}</title>
    </head>
    <body>
        <h1>{{ title }}</h1>
        <a href="{{ link_to_add }}">Add Feed</a>
        <table>
            <tr> <th>Name</th> <th>Created</th> </tr>
            {% for feed in items %}
                <tr>
                    <td> <a href="{{ link_to_show ~ feed.getAttribute('id') }}">{{ feed.getAttribute('name') }}</a> </td>
                    <td> {{ feed.getAttribute('created_at').diffForHumans() }} </td>
                </tr>
            {% endfor %}
        </table>
    </body>
</html>